datalen|id|message|signsignä¸º4bytes:    //set messageBody    Array.Copy( message, 0, sendData, 4, message.Length);    //set secret key for calculate Md5 sign    Array.Copy(SECRET_KEY, 0, sendData, 4+message.Length, 4);    byte[] md5 = CryptoHelper.MD5Sign( sendData );    //retrive 4 bytes of md5 as sign    Array.Copy(md5, 4, sendData,  4+message.Length,  1); //append 1 byte    Array.Copy(md5, 6, sendData,  4+message.Length+1, 1); //append 1 byte    Array.Copy(md5, 8, sendData,  4+message.Length+2, 1); //append 1 byte    Array.Copy(md5, 10, sendData, 4+message.Length+3, 1); //append 1 byte    byte[] SECRET_KEY = new byte[] { 0x93, 0x46, 0x78, 0x20 };    signData=datalen|id|message+SECRET_KEY    sign = md5(signData)